<div class="starter template move" >
<p class="lead moveWel"> what is happening</p>
</div>

<div class="col-md-6 col-md-offfset-3 move">
<form id="sendTweet">
<input type="text" class="form-control" id="tweet">
<br>
<button class="btn btn-primary">Tweet</button>
</form>
<br>
<br>

<div id="tweets">
    <%tweets.forEach(tweet =>{%>

        <div class="media">
            <div class="media-left">
                <a href="/user/<%=tweet.owner._id%>"><img class="media-object"  src="<%=tweet.owner.photo%>"  alt=""></a>
            </div>
                <div class="media-body yes">
                    <h4 class="media-heading"><%=tweet.owner.name%></h4>
                    <p><%=tweet.content%></p>
                </div>
            </div>
    <%})%>;
    
   
 
</div>


</div>